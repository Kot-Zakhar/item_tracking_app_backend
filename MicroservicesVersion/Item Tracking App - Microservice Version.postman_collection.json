			"item": [
				{
					"name": "Sign In",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Fixed ReferenceError by removing undefined variable",
									"var responseBody = pm.response.json();",
									"pm.collectionVariables.set(\"access_token\", responseBody.accessToken);",
									"",
									"var requestBody = JSON.parse(pm.request.body);",
									"pm.collectionVariables.set(\"fingerprint\", requestBody.fingerprint);",
									"",
									"pm.test(requestBody.fingerprint)"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"admin@gmail.com\",\n    \"password\": \"admin@1234\",\n    \"fingerprint\": \"{{$guid}}\",\n    \"userAgent\": \"test-useragent\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{identity_service_address}}/api/auth/sign-in",
